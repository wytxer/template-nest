{
  "name": "template-nestjs-standalone",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "pnpm run dev:local",
    "dev:local": "cross-env NODE_ENV=local nest start --watch",
    "dev:test": "cross-env NODE_ENV=test nest start --watch",
    "start": "cross-env NODE_ENV=online node dist/main",
    "pm2:start": "pm2 start --name \"template-nestjs-standalone\" pnpm -- run start",
    "pm2:reload": "pm2 reload template-nestjs-standalone",
    "pm2:stop": "pm2 stop template-nestjs-standalone",
    "start:test": "cross-env NODE_ENV=test node dist/main",
    "pm2:start:test": "pm2 start --name \"template-nestjs-standalone-test\" pnpm -- run start:test",
    "pm2:reload:test": "pm2 reload template-nestjs-standalone-test",
    "pm2:stop:test": "pm2 stop template-nestjs-standalone-test",
    "start:debug": "cross-env NODE_ENV=test nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "6.1.2",
    "axios": "^1.3.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.1.0",
    "lodash.merge": "^4.6.2",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "nestjs-pino": "^3.1.2",
    "nestjs-wechat-open-api": "^0.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^9.4.0",
    "qiniu": "^7.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "sequelize": "^6.29.1",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "28.1.4",
    "@types/node": "^18.14.1",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "28.1.2",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "author": "wytxer",
  "license": "",
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "prettier --write",
    "./src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}